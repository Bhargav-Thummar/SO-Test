<div class="d-flex py-2">
  <div class="flex-grow-1">
    <h1>Users</h1>
  </div>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th>Full Name</th>
      <th>Email</th>
      <th>Is Admin?</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td>
          <%= user.full_name %>
        </td>
        <td>
          <%= user.email %>
        </td>
        <td>
          <%= user.is_admin? %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
